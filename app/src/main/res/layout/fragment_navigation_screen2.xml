<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             xmlns:app="http://schemas.android.com/apk/res-auto"
             android:layout_width="match_parent" android:layout_height="match_parent"
             tools:context=".androidCore.jetpack.navigation.NavigationScreen2Fragment" android:id="@+id/frameLayout">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <TextView
                android:id="@+id/text_view_screen_2_title"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:text="@string/android_core_screen_jetpack_navigation_screen_2"
                android:gravity="center"
                style="@style/HeaderTextStyle"
                android:layout_marginTop="8dp"/>

        <TextView
                android:id="@+id/text_view_screen_2_received_info"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:text="@string/android_core_screen_jetpack_navigation_screen_2_reveived_text"
                android:gravity="center"
                style="@style/SimpleTextStyle"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/text_view_screen_2_title"/>

        <TextView
                android:id="@+id/text_view_screen_2_received"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                tools:text="@string/android_core_screen_jetpack_navigation_screen_2_reveived_text"
                android:gravity="center"
                style="@style/HeaderTextStyle"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/text_view_screen_2_received_info"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>